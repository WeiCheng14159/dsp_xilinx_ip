   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "n = np.arange(30) - 15\n",
    "phi = np.pi*n/2\n",
    "\n",
    "h = np.zeros(np.size(n))\n",
    "for k in range(np.size(n)):\n",
    "    if phi[k] == 0:\n",
    "        h[k] = 0\n",
    "    else:\n",
    "        h[k] = 2*np.sin(phi[k])**2 / (phi[k]*2)\n",
    "\n",
    "plt.figure()\n",
    "plt.stem(h, '.-b')\n",
    "plt.grid()"
   ]
  },
